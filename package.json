    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-testing-library": "^5.10.3",
    "eslint-plugin-vue": "^9.11.0",
    "happy-dom": "^9.10.9",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "unocss": "^0.51.12",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.5",
    "vite-plugin-pages": "^0.29.0",
    "vite-plugin-progress": "^0.0.7",
